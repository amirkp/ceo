\begin_layout Itemize
CEO's Valuation: 
\begin_inset Formula $2.3\text{HHI}+0.5\text{SIZE}-0.3\text{GAI}\times\text{SIZE}+1.3\text{GAI\ensuremath{\times}}\eta-4.3\xi$
\end_inset


\end_layout

\begin_layout Itemize
Firm's Valuation: 
\begin_inset Formula $4.0\text{GAI}+0.9\text{GAI}\times\text{HHI}+1.1\text{GAI }\times\text{SIZE}+0.5\varepsilon\eta$
\end_inset

 
\end_layout

\begin_layout Itemize
Let 
\begin_inset Formula $\text{GAI}=1.5$
\end_inset

, 
\begin_inset Formula $\text{HHI}=0.25$
\end_inset

, 
\begin_inset Formula $\text{SIZE}=2$
\end_inset

, 
\begin_inset Formula $\eta=0$
\end_inset

, 
\begin_inset Formula $\varepsilon=.5$
\end_inset

, 
\begin_inset Formula $\xi^{d}=1$
\end_inset

 which are approximately the median value for each characteristic
\end_layout

\begin_layout Itemize
CEO's valuation: -
\begin_inset Formula $3.10$
\end_inset

, Firm's valuation: 
\begin_inset Formula $9.11$
\end_inset

, Median Firm Profit = 4.4 
\end_layout

