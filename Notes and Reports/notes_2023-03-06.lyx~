\begin_deeper
\begin_layout Itemize
\begin_inset Formula 
\[
x:\quad\text{GAI; }\quad y_{1}:\quad\text{HHI;}\quad y_{2}:\quad\text{Size;}
\]

\end_inset


\end_layout

\begin_layout Itemize
CEO's Valuation
\begin_inset Formula 
\[
\beta_{xy_{2}}^{u}xy_{2}+\beta_{x\eta}^{u}x\eta
\]

\end_inset


\end_layout

\begin_layout Itemize
Firm's Valuation
\begin_inset Formula 
\[
\beta_{y_{1}}^{d}y_{1}+\beta_{y_{2}}^{d}y_{2}+\beta_{xy_{1}}^{d}xy_{1}+\beta_{xy_{2}}^{d}xy_{2}+\beta_{\varepsilon\eta}^{d}\varepsilon\eta
\]

\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula 
\[
\varepsilon\sim Uniform(0,1)\quad\eta\sim LogNormal(\mu=0,\sigma_{\eta})
\]

\end_inset


\end_layout

\begin_layout Itemize
Linear terms in 
\begin_inset Formula $y_{1}$
\end_inset

 and 
\begin_inset Formula $y_{2}$
\end_inset

 only affect downstream profits.
 Not identified, even when median downstream profit is fixed.
 
\end_layout

\end_deeper
