\begin_deeper
\begin_layout Itemize
CEO's Valuation
\begin_inset Formula 
\[
\beta_{y_{1}}^{u}y_{1}+\beta_{xy_{2}}^{u}xy_{2}+\beta_{y_{2}}^{u}y_{2}+\beta_{x\eta}^{u}x\eta+\xi^{d}
\]

\end_inset


\end_layout

\begin_layout Itemize
Firm's Valuation
\begin_inset Formula 
\[
\beta_{x}^{d}x+\beta_{xy_{1}}^{d}xy_{1}+\beta_{xy_{2}}^{d}xy_{2}+\beta_{\varepsilon\eta}^{d}\varepsilon\eta
\]

\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula 
\[
\varepsilon\sim Uniform(0,1);\,\eta\sim Normal(\mu=0,\sigma_{\eta});\,\xi^{d}\sim Normal(\mu=0,\sigma_{\xi})
\]

\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\xi^{d}$
\end_inset

 is an unobserved firm specific shock to CEO's valuation.
 Needed to better fit the variations in prices.
\end_layout

\end_deeper
